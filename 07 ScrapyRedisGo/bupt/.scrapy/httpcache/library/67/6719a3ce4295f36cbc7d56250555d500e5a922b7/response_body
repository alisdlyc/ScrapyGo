<script src="pages/dist/jquery/jquery.min.js"></script>
<script src="pages/dist/bootstrap/js/bootstrap.min.js"></script>
<script src="pages/j/jquery.easydropdown.js"></script>
<script src="pages/j/icheck.min.js"></script>
