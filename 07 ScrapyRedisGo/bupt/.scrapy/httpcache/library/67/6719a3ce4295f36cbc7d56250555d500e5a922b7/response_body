<script src="pages/dist/jquery/jquery.min.js"></script>
<script type="text/javascript">
    $(function () {
        $(".plus").each(function () {
            $(this).click(function () {
                var name = $(this).attr("src").split("/").pop().split(".").shift();
                if (name == "plus") {
                    $(this).attr({
                        src: 'pages/i/minus.png',
                    });
                } else if (name == "minus") {
                    $(this).attr({
                        src: 'pages/i/plus.png',
                    });
                }
            });
        });
        $(".book-cover").each(function () {
            $(this).mouseover(function () {
                $(this).parent().parent().find(".book-info").show(50);
            });
            $(this).mouseout(function () {
                $(this).parent().parent().find(".book-info").hide(50);
            });
        });
    });
</script>

