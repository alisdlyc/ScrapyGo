<script type="text/javascript">
    $(function () {
        $(".book-list").on('mouseenter', '.book-hd a', function(event) {
            if(event.type == 'mouseenter'){
                var obj = this;
                var sid=$(obj).parent().attr("id");
//                var sid=$(this).parent().attr("id");
                var params={sid:sid};
                var showBox = "";
                $.ajax({
                    url:"http://opac.bupt.edu.cn:8080//search_info.json",
                    type:"post",
                    data:params,
                    success:function(data){

                        if(data.result.code==0){

                                var html = "";
                                var item = data.data;
                                html +='<h3>'+item.title+'</h3>';
                                html +='<p>'+item.authors+'／'+item.pubdateDate+'／'+item.publisher+'</p>';
                                html +='<p>标准书号：<span class="space">'+((item.isn==null||item.isn=='')?"无":item.isn)+'</span></p>';
                            

                            html +='<p>索书号：<span class="space">'+((item.bookSearchNo==''||item.bookSearchNo==null)?"":item.bookSearchNo)+'</span></p>';
                            
                                html +='<ul class="list-inline">';
                                var terms = item.termList;
                                for(var j=0;j<terms.length;j++){
                                    html +='<li class="catogary">'+terms[j]+'</li>';
                                }
                                html +='</ul>';
                                html +='<ul class="list-inline grade">';
                                html +='<li>评分：';
                                html +='<span>';
                                for(var j=1;j<=item.reGrade;j++){
                                    html +='<img src="pages/i/star_active.png"/>';

                                }
                                for(var j=1;j<=(5-item.reGrade);j++){
                                    html +='<img src="pages/i/star_normal.png"/>';

                                }
                                html +='</span>';
                                html +='</li>';
                                if(item.kejieCount > 0){
                                    html +='<li>在架可借</li>';
                                }else{
                                    html +='<li>不可借</li>';
                                }
                                html +='</ul>';
                                showBox = $('<div class="book-info"></div>');
                                showBox.append(html);  //添加头部
//                            $(".book-list").on('mouseenter', '.book-hd a', function(event) {
                                if(event.target == obj){
                                    $(obj).parent().parent().parent().append(showBox);
                                }

//                            })
                            $(".book-list").on('mouseleave', '.book-hd a', function(event) {
                                if(event.type == 'mouseleave'){
                                    $(obj).parent().parent().parent().find(".book-info").remove();
                                }
                            });

                        }
                    }
                });
            }

        });
        $(".book-list").on('mouseleave', '.book-hd a', function(event) {
            if(event.type == 'mouseleave'){
                $(this).parent().parent().parent().find(".book-info").remove();
            }
        });
        $(".book-list").on('mouseenter', '.book-info', function(event) {
            if(event.type == 'mouseenter'){
                $(this).show();
            }
        });
        $(".book-list").on('mouseleave', '.book-info', function(event) {
            if(event.type == 'mouseleave'){
                $(this).remove();
            }
        });
    })
</script>


</body>
</html>
