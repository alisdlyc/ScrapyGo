<script src="pages/sweetalert-master/docs/assets/sweetalert/sweetalert.min.js"></script>
<script type="text/javascript">

    $(function () {
        
        createCode();
    });

    function login() {
        var inputCode = document.getElementById("input1").value.toUpperCase();
        var inputUserLoginName = document.getElementById("account").value;
        var inputUserPassword = document.getElementById("password").value;
        if (inputCode.length <= 0) {
            alert("请输入验证码！");
            return false;
        } else if (inputCode != code) {
            alert("验证码输入错误！");
            createCode();
            return false;
        }
        var params = {};
        params.login_type = $("#login_type").val();
        params.account = $("#account").val();
        params.password = $("#password").val();
        var reu = loginsuccess(params);
        if (reu == 1) {
            //window.location.href="http://opac.bupt.edu.cn:8080/reader-setting.html";
            window.location.reload();
        }
        return false;
    }

    function loginsuccess(params) {
        var reu = 0;
        $.ajax({
            url: "http://opac.bupt.edu.cn:8080//logon.json",
            type: "post",
            async: false,
            data: params,
            success: function (data) {
                if (data.result.code == 0) {
                    reu = 1;
                }else if(data.result.code == 2){
                    alert(data.result.msg);
                    window.location.href = "http://opac.bupt.edu.cn:8080/fixPassWord.html";
                } else {
                    alert(data.result.msg);
                }
            }

        });
        return reu;
    }

    function anonymous() {
        var params = {};
        params.name = $.trim($("#name").val());
        if (params.name == '') {
            alert("姓名不能为空！");
            return;
        }
        params.phone = $.trim($("#phone").val());
        var test = /^1[3-9]\d{9}$/;
        if (params.phone == "" || !test.test(params.phone)) {
            alert("联系电话格式不对！");
            return;
        }
        params.email = $.trim($("#email").val());
        $.ajax({
            url: "http://opac.bupt.edu.cn:8080//anonymous.json",
            type: "post",
            async: false,
            data: params,
            success: function (data) {

                if (data.result.code == 0) {
                    window.location.href = "http://opac.bupt.edu.cn:8080/reader-setting.html";
                }
            }

        });
        return false;
    }

    function clearText() {
        document.getElementById('name').value = "";
        document.getElementById('phone').value = "";
        document.getElementById('email').value = "";
    }

    function findPwd() {
        var params = {};
        params.readerId = $.trim($("#readerId").val());
        if (params.readerId == '') {
            alert("用户号不能为空！");
            return;
        }
        params.readerEmail = $.trim($("#readerEmail").val());
        var test = /^[\w!#$%&'*+/=?^_`{|}~-]+(?:\.[\w!#$%&'*+/=?^_`{|}~-]+)*@(?:[\w](?:[\w-]*[\w])?\.)+[\w](?:[\w-]*[\w])?$/;
       /* if (!test.test(params.readerEmail)) {
            alert("保密邮箱不符合规则！");
            return;
        }*/
        $.ajax({
            url: "http://opac.bupt.edu.cn:8080//findPwd.json",
            type: "post",
            async: false,
            data: params,
            success: function (data) {
                if (data.result.code == 0) {
                    alert("邮件发送成功，请查收！");
                } else {
                    alert(data.result.msg);
                }
            }
        });
    }
    function bigpage() {
        var big = $("#total_page").text();
        var jump = $("#jumpnum").val();
        if (parseInt(jump) > parseInt(big)) {
            alert("当前结果最大页数为" + big + "页,请不要超过最大页数！");
            return -1;
        }
        return 1;
    }
</script>
<script type="text/javascript">
    var link = window.location.href.split("/").pop();
    link = link.substring(0, link.indexOf(".html"));
    link += ".html";
    $(".nav-item").each(function () {
        var href = $(this).attr("href");
        if (link == href) {
            $(this).parent().addClass("active");
        }
    });
</script>
<script>
    var lastTime = new Date().getTime();
    var currentTime = new Date().getTime();
    var timeOut = 20 * 60 * 1000; //设置超时时间： 10分
    $(function () {
        /* 鼠标移动事件 */
        $(document).mouseover(function () {
            lastTime = new Date().getTime(); //更新操作时间

        });
    });
    function testTime() {
        var reader =
        if (reader == undefined) {
            return;
        }
        currentTime = new Date().getTime(); //更新当前时间
        if (currentTime - lastTime > timeOut) {   //判断是否超时
            alert("长时间未操作，请重新登陆");
            window.location.href = 'http://opac.bupt.edu.cn:8080/logout.html';
        }
    }
    /* 定时器  间隔1秒检测是否长时间未操作页面  */
    window.setInterval(testTime, 1000);
</script>




<!--/footer-->

<script type="text/javascript">
    $(function () {
        init("A", 1);
    });

    var t = "";
    var d = "";
    var order = -1

    function classm(classnoAbs) {
        t = classnoAbs;
        init(t, 1);
    }

    function ordersm(obj, m) {
        order = obj;
        $("#order").html(m + '<span class="caret"></span>');
        init(t, 1);
    }

    var page = 1;

    function init(classnoAbs, pageNo) {
        page = pageNo;
        t = classnoAbs;
        var pageSize = $("#page-size").val();
        var params = {classnoAbs: classnoAbs, pageNo: pageNo, pageSize: pageSize, order: order};

        $.ajax({
            url: "http://opac.bupt.edu.cn:8080//search-classify.json",
            type: "post",
            data: params,
            success: function (data) {

                if (data.result.code == 0) {
                    $("#total").text(data.total);
                    topage(data.data, data.pageNo, data.totalPage, pageSize);
                }
            }

        });
    }

    function search_reclist(sid) {
        var params = {sid: sid};

        $.ajax({
            url: "http://opac.bupt.edu.cn:8080//search_reclist.json",
            type: "post",
            data: params,
            success: function (data) {

                if (data.result.code == 0) {
                    var html = "";
                    for (var i = 0; i < data.data.length; i++) {
                        var item = data.data[i];
                        html += '<a href="bookInfo_';
                        html += item.recCtrlId;
                        html += '.html"><li>';
                        html += item.title + '</li></a>';
                    }
                    $("#reclist").html(html);
                }
            }

        });
    }

    function topage(data, page, totalPage, pageSize) {
        var html = "";
//        alert(data.length);
        for (var i = 0; i < data.length; i++) {
            var item = data[i];
            html += '<tr>';
            html += '<td>';
            html += '<img src="' + item.imgUrl + '" class="book-cover"/>';
            html += '</td>';
            html += '<td colspan="3">';
            html += '<p class="book-hd" id="' + item.recCtrlId + '"><a href="bookInfo_' + item.recCtrlId + '.html"  target="_blank">' + item.title + '</a>';
            if (item.recCtrlId.indexOf("1") == 0) {
                html += '<span class="books-icon">期刊</span></p>';
            } else {
                html += '<span class="books-icon">图书</span></p>';
            }
            html += '<p class="book-intro">' + ((item.authors == null || item.authors == '') ? "无" : item.authors) + '／' + ((item.pubdateDate == null || item.pubdateDate == '') ? "无" : item.pubdateDate) + '／' + ((item.publisher == '' || item.publisher == null) ? "无" : item.publisher) + '</p>';
            html += '<p class="book-intro">标准书号：<span class="space">' + ((item.isn == null || item.isn == '') ? "无" : item.isn) + '</span>';
            html += '</p>';
//            html +='<p class="book-intro">';
//            html +='借阅次数：<span class="space">'+item.lendCount+'</span>';
//            html +='查看次数：<span class="space">'+item.lookCount+'</span>';
//            html +='收藏次数：<span class="space">'+item.collectCount+'</span>';
//            html +='</p>';
            html += '</td>';
            html += '</tr>';
            if (i == 0) {
                search_reclist(item.recCtrlId);
            }
        }
        $("#list").html(html);
        var pageHtml = "";
        if (page > 1) {
            pageHtml += '<li><a href="javascript:;" onclick="init(t,' + (page - 1) + ')">上一页</a></li>';
        }
        var start = page - 2 < 1 ? 1 : page - 2;
        if (page == totalPage - 1) {
            start = page - 3 < 1 ? 1 : page - 3;
        }
        if (page == totalPage) {
            start = page - 4 < 1 ? 1 : page - 4;
        }
        var end = page + 2 > totalPage ? totalPage : page + 2;
        if (page == 1) {
            end = page + 4 > totalPage ? totalPage : page + 4;
        }
        if (page == 2) {
            end = page + 3 > totalPage ? totalPage : page + 3;
        }

        for (var i = start; i <= end; i++) {

            if (i == page) {
                pageHtml += '<li class="active"><a href="javascript:;" >' + i + '</a></li>';
            } else {
                pageHtml += '<li><a href="javascript:;" onclick="init(t,' + i + ')">' + i + '</a></li>';
            }
        }
        if (page < totalPage) {
            pageHtml += '<li><a href="javascript:;" onclick="init(t,' + (page + 1) + ')">下一页</a></li>';
        }
        $("#page").html(pageHtml);
        $("#total_page").html(totalPage);

    }

    function jumppage() {
        if (bigpage() == -1) {
            return;
        }
        var p = $.trim($("#jumpnum").val());
        init(t, p);
    }

    function load_pagesize() {
        init(t, page);
    }
</script>

